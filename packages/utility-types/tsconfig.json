{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "@any/*": ["any/*"],
      "@any": ["any"],
      "@logic/*": ["logic/*"],
      "@logic": ["logic"],
      "@math/*": ["math/*"],
      "@math": ["math"],
      "@object/*": ["object/*"],
      "@object": ["object"],
      "@template-literal/*": ["teplate-literal/*"],
      "@template-literal": ["teplate-literal"],
      "@tuple/*": ["tuple/*"],
      "@tuple": ["tuple"],
      "@prop/*": ["prop/*"],
      "@prop": ["prop"],
      "@identity/*": ["identity/*"],
      "@identity": ["identity"]
    }
  },
  "include": ["src"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
